    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "postcss-px-to-viewport": "^1.1.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "sass": "^1.37.0",
    "typescript": "^4.3.2",
    "vconsole": "^3.9.1",
    "vite": "^2.4.4",
    "vue-tsc": "^0.2.2"
